---
var head_title = Astro.props.head_title;
if (!head_title) {
    head_title = Astro.props.frontmatter.head_title;
}
import Env_Script from "../components/env_script.astro";
import Loading from "../components/loading.astro";
import Nav_Bar from "../components/nav_bar.astro";
---

<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link
            rel="stylesheet"
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.css"
        />
        <link rel="stylesheet" href="/assets/main.css" />
        <link
            rel="stylesheet"
            href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined"
        />
        <script
            is:inline
            async
            src="https://www.googletagmanager.com/gtag/js?id=G-YS5RWDRN7N"
        ></script>
        <script is:inline>
            window.dataLayer = window.dataLayer || [];
            function gtag() {
                dataLayer.push(arguments);
            }
            gtag("js", new Date());

            gtag("config", "G-YS5RWDRN7N");
        </script>
        <script
            is:inline
            src="https://js.sentry-cdn.com/b0b31aaf38de4cfaa4e02a717b357fac.min.js"
            crossorigin="anonymous"></script>
        <title>{head_title}</title>
        <noscript>
            <title>Fantasy Security</title>
        </noscript>
    </head>
    <body>
        <Env_Script />
        <Loading />
        <div id="root" class="rootloading" hidden>
            <Nav_Bar />
            <div id="main">
                <slot />
            </div>
        </div>
        
    </body>
    <noscript>
        <div
            style="overflow:hidden;overflow-x:hidden;overflow-y:hidden;height:100%;width:100%;position:absolute;top:0%;left:0px;right:0px;bottom:0px"
        >
            <center><h1>Turn on JavaScript</h1></center>
        </div>
    </noscript>
    <script
        is:inline
        src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    ></script>
    <script src="../js/init.js"></script>
</html>
